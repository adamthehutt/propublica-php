{
  "name": "adamthehutt/propublica-php",
  "description": "A simple PHP client for Propublica's API",
  "minimum-stability": "dev",
  "type": "library",
  "require": {
    "php": ">=7.3",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.2.1"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Adam Huttler",
      "email": "adam.huttler@gmail.com"
    }
  ],
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "AdamTheHutt\\ProPublica\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AdamTheHutt\\ProPublica\\Tests\\": "tests/"
    }
  }
}
